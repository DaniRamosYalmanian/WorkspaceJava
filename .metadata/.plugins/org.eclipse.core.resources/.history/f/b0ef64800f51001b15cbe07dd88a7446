
public class Carro {
	String marcaModelo;
	int ano;
	String cor;
	double preco;
	int km;
	int finalPlaca;
	
	void exibirAnuncio() {
		System.out.println("**************************************************");
		System.out.println("Automóvel ");
		System.out.println("Marca/Modelo:" + marcaModelo + " - Ano: " + ano);
		System.out.println("Cor" + cor);
		System.out.println("Final Placa:" + finalPlaca + " KM: " + km);
		System.out.println("Preco: " + preco);
		System.out.println("**************************************************");
		
	}
}
