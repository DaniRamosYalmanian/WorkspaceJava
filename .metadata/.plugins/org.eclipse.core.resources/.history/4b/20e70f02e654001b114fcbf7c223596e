public class ListaFuncionarios {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	 Funcionario func1, func2, func3, func4;
	 
	 func1 = new Chefe("Leonardo Caldeira",1,1000,1000,1000);
	 func2 = new Comissionado("Daniela Ramos",2,1000,1000);
	 func3 = new Empreteiro("Carlos Melchior",3,1000);
